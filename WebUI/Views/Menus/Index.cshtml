@model IEnumerable<WebUI.Models.Menu>
@{
    ViewData["Title"] = "Menus";
}
<h2>@ViewData["Title"]</h2>
<p><a asp-action="Create" class="btn btn-primary">Create</a></p>
<table class="table table-striped">
  <thead><tr><th>Name</th><th>Url</th><th>Order</th><th>Date</th></tr></thead>
  <tbody>
    @foreach (var m in Model)
    {
      <tr>
        <td>@m.Name</td>
        <td>@m.Url</td>
        <td>@m.Order</td>
        <td>@m.Date.ToString("dd.MM.yyyy")</td>
      </tr>
    }
  </tbody>
</table>
