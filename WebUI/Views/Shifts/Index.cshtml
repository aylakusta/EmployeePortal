@model IEnumerable<WebUI.Models.Shift>
@{
    ViewData["Title"] = "Shifts";
}
<h2>@ViewData["Title"]</h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Date</th>
      <th>Start</th>
      <th>End</th>
      <th>Type</th>
    </tr>
  </thead>
  <tbody>
  @foreach (var s in Model)
  {
    <tr>
      <td>@s.Date.ToString("dd.MM.yyyy")</td>
      <td>@(s.StartTime?.ToString(@"hh\:mm"))</td>
      <td>@(s.EndTime?.ToString(@"hh\:mm"))</td>
      <td>@s.ShiftType</td>
    </tr>
  }
  </tbody>
</table>
